<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
     <style name="fill">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">fill_parent</item>
    </style>
    <style name="fill_width">
         <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
    </style>
    <style name="fill_width_partial" parent="fill_width">
        <item name="android:layout_marginLeft">10dip</item>
        <item name="android:layout_marginRight">10dip</item>
    </style>
    
    <style name="wrap">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    
    <style name="imagebutton" parent="wrap">
        <item name="android:padding">5dip</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    
    <style name="ics_button" parent="wrap">
        <item name="android:background">@drawable/appwidget_dark_bg_clickable</item>
        <item name="android:textColor">@color/androidblue</item>
    </style>
    
    <style name="splash_button" parent="fill_width">
        <item name="android:textColor">@color/forestgreen</item>
        <item name="android:textSize">20dip</item>
        <item name="android:paddingTop">15dip</item>
        <item name="android:paddingBottom">15dip</item>
    </style>
    
    <style name="margin_leftright" parent="imagebutton">
        <item name="android:layout_marginLeft">10dip</item>
        <item name="android:layout_marginRight">10dip</item>
    </style>
    
    <style name="underline" parent="wrap">
        <item name="android:background">@color/lightgreen</item>
        <item name="android:layout_height">4dip</item>
    </style>
    
</resources>